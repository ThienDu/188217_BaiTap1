<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />

    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>
   
    <div class="container">
        <main role="main" >
            @RenderBody()
        </main>
    </div>

    

    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
